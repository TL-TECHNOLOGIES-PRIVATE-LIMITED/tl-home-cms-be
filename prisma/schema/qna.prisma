model FAQ {
  id        String   @id @default(uuid())
  question  String   @db.Text
  answer    String   @db.Text
  order     Int      @db.Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model PrivacyPolicy {
  id        String   @id @default(uuid())
  title     String
  content   String   @db.Text
  order     Int      @db.Int
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
} 